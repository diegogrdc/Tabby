Program matsOk; 

Var Arr Int a[10][10];
Var Int i, j;

Tabby() {
    a[1][1] = 1;
    a[1][2] = 2;
    a[1][3] = 3;
    a[2][1] = 4;
    a[2][2] = 5;
    a[2][3] = 6;
    a[3][1] = 7;
    a[3][2] = 8;
    a[3][3] = 9;

    j = 3;
    For(j >= 1; j = j - 1) {
        i = 1;
        For(i <= 3; i = i + 1) {
            Write(a[i][j], " ");
        }
        Write("\n");
    }
}