Program statisticsDiffType; 

Var Arr Int a[1005];
Var Arr Int b[1005];
Var Int n, i;

Tabby() {
    n = 500;
    i = 0;
    For(i < n; i = i + 1) {
        a[i] = i;
        b[i] = 3 * i - i * i;
    }
    HistogramPlot(a, 100, n, "hist");
    LinePlot(a, b, n, "line");
    ScatterPlot(a, b, n, "scatter");
}