Program testQuadsNonLinear;

Var Bool i;

Tabby() {
    Write("ProgramStt");
    For(i == True; i = True) {
        Write("ForStt");
        If(i == True) {
            Write("IfStt");
            While(i == True) {
                Write("While");
            }
            Write("ExitWhile");
            Write("IfEnd");
        } Else {
            Write("ElseStt");
            If(i == False) {
                Write("If2");
            }
            Write("ExitIf2");
            Write("ElseEnd");
        }
        Write("ForEnd");
    }
    Write("ProgramEnd");
}