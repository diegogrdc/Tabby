Program tabbyMatrixRead;

Var Int i, j, n;
Var Arr Int mat[50][50];

Fn Void statsMedian(Arr Int arr) {
    Return arr[1][1];
}

Tabby() {
    Read(n);
    i = 1;
    For(i <= n; i = i + 1) {
        j = 1;
        For(j <= n; j = j + 1) {
            Read(mat[i][j]);
        }
    }

    Write("Matrix was read and stored in memory", statsMedian(mat));
}